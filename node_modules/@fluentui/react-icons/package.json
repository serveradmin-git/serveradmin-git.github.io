    "clean:gitignore": "git clean -fdX",
    "clean:svg": "rm -rf ./intermediate",
    "convert:fonts": "node convert-font.js --source=./src/utils/fonts --dest=./src/fonts --codepointDest=./src/utils/fonts --rtl=./intermediate/rtl.json",
